{
  "name": "My Scraper",
  "description": "scraping test",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "page_actions/popup.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["content_scripts/main.js"],
    "run_at": "document_end"
  }],
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}
